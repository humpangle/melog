{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "member-access": false,
    "object-literal-sort-keys": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-any": true,
    "no-arg": true,
    "ordered-imports": false
  }
}
